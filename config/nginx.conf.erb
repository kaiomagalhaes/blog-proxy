worker_processes  2;
error_log /dev/stdout info;

daemon off;

events {
	worker_connections 256;
}

http {
	access_log /dev/stdout;
	server {
		listen <%= ENV["PORT"] %>;
		server_name blog-proxy.herokuapp.com/;
		proxy_ssl_server_name on;


		location / {
			proxy_pass https://dev-codelitt-blog.herokuapp.com/blog;
		}		
	}
}
